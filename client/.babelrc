{
  "presets": [
    "es2015",
    "stage-0",
    "react"
  ],
  "plugins": [
    [
      "module-alias",
      [
        {
          "src": "./app",
          "expose": "app"
        },
        {
          "src": "./test",
          "expose": "test"
        },
        {
          "src": "./app/helpers",
          "expose": "helpers"
        },
        {
          "src": "./app/components",
          "expose": "components"
        },
        {
          "src": "./app/schemas",
          "expose": "schemas"
        },
        {
          "src": "./app/sources",
          "expose": "sources"
        },
        {
          "src": "./app/models",
          "expose": "models"
        },
        {
          "src": "npm:lodash",
          "expose": "underscore"
        }
      ]
    ]
  ]
}
